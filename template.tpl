___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const copyFromWindow = require('copyFromWindow');
const setInWindow = require('setInWindow');
const injectScript = require('injectScript');

// create pre load queue
const objectName = 'ce';
setInWindow('CrossEngageObject', objectName, true);
const ceObject = copyFromWindow(objectName) || function() {
  const currObj = copyFromWindow(objectName);
  (currObj.q = currObj.q || []).push(arguments);
  setInWindow(objectName, currObj, true);
};
setInWindow(objectName, ceObject, true);

// SDK source script injection
let sdkUrl = 'https://app.crossengage.io/ce.js';
if(data.customSdkUrl) {
  sdkUrl = data.customSdkUrl;
}
injectScript(sdkUrl, data.gtmOnSuccess, data.gtmOnFailure, 'ceSDK');

const config = {
  apiKey: data.apiKey,
  silentLogs: data.silentLogs
};

// Advanced Options
if (data.useRootDomain) {
  config.rootDomain = (data.rootDomain || true);
}
if (data.customApiUrl) {
  config.apiUrl = data.customApiUrl;
}
if (data.trackingOptIn) {
  config.trackingOptIn = true;
}

ceObject(config);

if (data.autoPageView) {
  ceObject('track', 'Viewed Page');
}


